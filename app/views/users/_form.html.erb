<div class='well'>
<%= form_for  @user do |f| %>
<%= render 'shared/errors_display', obj: @user %>
  <div class='control-group'>
  <%= f.label :username %>
  <%= f.text_field :username %>
  </div>
  <div class='control-group' >
  <%= f.label :password %>
  <%= f.password_field :password %>
  </div>
  <%= f.submit @user.new_record? ? "Register" : "Upadte Profile" ,class: 'btn btn-success'%>
<% end %>
</div>